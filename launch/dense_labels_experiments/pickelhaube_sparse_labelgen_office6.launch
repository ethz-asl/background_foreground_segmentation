<?xml version="1.0" encoding="utf-8"?>
<!--
Launch file for Selective ICP with rviz and Realsense integration
-->

<launch>
	<arg name="rosbag_folder" default="$(find background_foreground_segmentation)/rosbags" />
	<include file="$(find background_foreground_segmentation)/launch/dense_labels_experiments/pickelhaube2_labelgen_sparse_office.launch">
		<arg name="rosbag" value="$(arg rosbag_folder)/pickelhaube_2021-02-18-11-29-33_office6.bag"/>
		<arg name="cad_file" value="$(find background_foreground_segmentation)/launch/paper_experiments/191130_286_lee_j_vf_20160101_SE.off" />
		<arg name="logprefix" value="pickelhaube_dense_full_test" />
	</include>

    <node pkg="tf2_ros"
	      type="static_transform_publisher"
		  name="mesh_positioning"
		  args="-1.387 -9.907 -0.912 0 0 -0.686 0.728 map marker" />
    <node pkg="rosservice"
	      type="rosservice"
		  name="start_icp"
		  args="call --wait /mapper/load_published_map"
		  launch-prefix="bash -c 'sleep 2; $0 $@'" />

</launch>

