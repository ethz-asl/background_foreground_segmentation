<?xml version="1.0" encoding="utf-8"?>
<!--
Launch file for Selective ICP with rviz
-->

<launch>


	<include file="$(find background_foreground_segmentation)/launch/rss_experiments/pickelhaube2_segmentation.launch">
		<arg name="rosbag" value="/media/blumh/ext_2tb/pickelhaube_2021-02-18-11-29-33.bag" />
		<arg name="cad_file" value="$(find background_foreground_segmentation)/launch/rss_experiments/191130_286_lee_j_vf_20160101_SE.off" />
		<arg name="logprefix" value="pickelhaube_nyusegmentation_office6" />
		<arg name="segmentation_model" value="1ZF2nYMD9PE0kr435TrDVLpaHuDUBSOC5" />
		<arg name="input_height" value="480" />
		<arg name="input_width" value="640" />
		<arg name="threshold" value="50" />
	</include>

	<!--node name="mesh_positioning"
          pkg="cpt_selective_icp"
          type="position_cad_with_marker.py"
          output="screen">
    	<param name="marker_parent_frame" value="map" />
		<param name="marker_frame_name" value="marker" />
  	</node-->
    <node pkg="tf2_ros"
	      type="static_transform_publisher"
		  name="mesh_positioning"
		  args="-1.387 -9.907 -0.912 0 0 -0.686 0.728 map marker" />
    <node pkg="rosservice"
	      type="rosservice"
		  name="start_icp"
		  args="call --wait /mapper/load_published_map"
		  launch-prefix="bash -c 'sleep 2; $0 $@'" />

</launch>
