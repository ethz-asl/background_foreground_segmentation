<?xml version="1.0" encoding="utf-8"?>
<!--
Launch file for Selective ICP with rviz
-->

<launch>


	<include file="$(find background_foreground_segmentation)/launch/rss_experiments/pickelhaube2_online_learning.launch">
		<arg name="rosbag" value="/media/blumh/ext_2tb/pickelhaube_2021-02-08-12-41-26.bag" />
		<arg name="cad_file" value="$(find background_foreground_segmentation)/launch/rss_experiments/210212_Rumlang_Meters_SE.off" />
		<arg name="logprefix" value="pickelhaube_online_learning_rumlang2" />
		<arg name="segmentation_model" value="1Wu1p2U7BgK8TwzvTdLlBWCemo3oPN-ad" />
		<arg name="input_height" value="480" />
		<arg name="input_width" value="640" />
		<arg name="threshold" value="50" />
	</include>

    <node pkg="tf2_ros"
	      type="static_transform_publisher"
		  name="mesh_positioning"
		  args="14.505 -3.089 -1.667 0.000 0.000 1 0 map marker" />
    <node pkg="rosservice"
	      type="rosservice"
		  name="start_icp"
		  args="call --wait /mapper/load_published_map"
		  launch-prefix="bash -c 'sleep 2; $0 $@'" />

</launch>
