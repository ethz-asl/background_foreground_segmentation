<?xml version="1.0" encoding="utf-8"?>
<!--
Launch file for Selective ICP with rviz
-->

<launch>


	<include file="$(find background_foreground_segmentation)/launch/rss_experiments/pickelhaube2_full.launch">
		<arg name="rosbag" value="/media/blumh/ext_2tb/pickelhaube_2021-02-08-12-32-19.bag" />
		<arg name="cad_file" value="$(find background_foreground_segmentation)/launch/rss_experiments/210212_Rumlang_Meters_SE.off" />
		<arg name="logprefix" value="pickelhaube_full_rumlang1" />
	</include>

	<!--node name="mesh_positioning"
          pkg="cpt_selective_icp"
          type="position_cad_with_marker.py"
          output="screen">
    	<param name="marker_parent_frame" value="map" />
		<param name="marker_frame_name" value="marker" />
  	</node-->
    <node pkg="tf2_ros"
	      type="static_transform_publisher"
		  name="mesh_positioning"
		  args="14.505 -3.089 -1.667 0.000 0.000 0.999 -0.041 map marker" />
    <node pkg="rosservice"
	      type="rosservice"
		  name="start_icp"
		  args="call --wait /mapper/load_published_map"
		  launch-prefix="bash -c 'sleep 2; $0 $@'" />


</launch>
