  <node pkg="nodelet"
        type="nodelet"
        args="load image_proc/debayer /pickelhaube_kinect_camera_manager"
        name="kinect_grayscale" >
    <rosparam>
      decimation_x: 3
      decimation_y: 3
      height: 1280
      interpolation: 1
    </rosparam>
    <remap from="image_raw" to="/rgb/image_raw"/>
    <remap from="image_mono" to="/rgb/image_mono"/>
    <remap from="image_color" to="/rgb/image_color"/>
  </node>

  <node pkg="nodelet"
        type="nodelet"
        args="load image_proc/debayer /pickelhaube_kinect_camera_manager"
        name="cam2_grayscale" >
    <rosparam>
      decimation_x: 3
      decimation_y: 3
      height: 1280
      interpolation: 1
    </rosparam>
    <remap from="image_raw" to="/versavis/cam2/image_raw"/>
    <remap from="image_mono" to="/versavis/cam2/image_mono"/>
    <remap from="image_color" to="/versavis/cam2/image_color"/>
  </node>


