<launch>
      <param name="/use_sim_time" value="true"/>
    <node pkg="nodelet" type="nodelet" name="pickelhaube_kinect_camera_manager"
          args="manager"
          output="screen"
          required="true" >
       <param name="num_worker_threads" value="6" />
    </node>
